{
  "name": "boilerplate-components-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "kill-all": "pkill node",
    "start": "export NODE_ENV=development && node ./bin/www",
    "pm2-1": "export NODE_ENV=development && pm2 kill && pm2 start ./bin/www && pm2 monit",
    "pm2-2": "export NODE_ENV=development && pm2 kill && pm2 start ./bin/www -i 2 && pm2 monit",
    "pm2-max": "export NODE_ENV=development && pm2 kill && pm2 start ./bin/www -i max && pm2 monit",
    "pm2-kill": "pm2 kill",
    "local": "export NODE_ENV=development && nodemon ./bin/www",
    "test": "export NODE_ENV=test && nodemon --exec 'mocha  ./components/*/test/*/*.js ./'",
    "kill-5000": "sudo kill `sudo lsof -t -i:5000`",
    "kill-3000": "sudo kill `sudo lsof -t -i:3000`",
    "lint": "eslint ."
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "express": "~4.16.0",
    "http-errors": "~1.7.3",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mime": "^2.4.0",
    "mocha": "^8.3.0",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "validation-helpers": "^0.4.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.17.3",
    "phantomjs-prebuilt": "^2.1.16"
  }
}
